# Ignore node_modules in both frontend and backend
/frontend/node_modules/
/backend/node_modules/

# Ignore environment files in both frontend and backend
/frontend/.env
/backend/.env

# Ignore build directories (optional)
frontend/build/
/backend/dist/

# Ignore .DS_Store (Mac OS)
.DS_Store

# Ignore logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore the prisma folder with generated clients (optional, if you want to exclude prisma clients)
/backend/prisma/.client/

# Ignore any local IDE or editor config files (VSCode, etc.)
.vscode/
.idea/

# Ignore test coverage files
coverage/

# Ignore dependency folders
.pnp
.pnp.js

# Ignore parcel cache (if using parcel bundler)
/.cache
.parcel-cache

# Ignore eslint cache
.eslintcache

# Ignore typescript cache
*.tsbuildinfo
